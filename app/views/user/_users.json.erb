{
    "success": true,
    "message": "<%=t(:get_users_success)%>",
    "response": {
        "total_users": <%= @total_such %>,
        "users": [
            <% @users.each_with_index do |user, ind| %>
            {
                "id": <%= user.id %>,
                "username": "<%= user.username %>",
                "email": "<%= user.email %>",
                "registered": "<%= user.created_at.to_formatted_s(:ddmmyyyyhhmmss_format) %>",
                "banned": <%= user.banned %>,
                "privilege": "<%= user.user_privilege.name %>",
                "privilege_id": <%= user.user_privilege_id %>
            } <%= ',' if ind!=(@users.length-1) %>
            <% end %>
        ]
    }
}